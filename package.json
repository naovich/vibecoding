{
  "name": "vibecoding-template",
  "version": "1.0.0",
  "description": "VibeCoding template with all guardrails configured",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "validate": "npm run lint && npm run type-check && npm run test:run",
    "validate:full": "npm run validate && npm run test:coverage",
    "check:any": "node scripts/check-no-any.js",
    "check:file-size": "node scripts/check-file-size.js",
    "check:coverage": "node scripts/enforce-coverage.js"
  },
  "keywords": [
    "template",
    "typescript",
    "vitest",
    "eslint",
    "husky"
  ],
  "author": "Nadhoir ATTOUMANI",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "glob": "^11.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
